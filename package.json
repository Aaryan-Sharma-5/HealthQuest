{
  "name": "healthquest-monorepo",
  "version": "1.0.0",
  "description": "HealthQuest - Gamified Health Motivation Platform",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python app.py",
    "install:all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "keywords": [
    "health",
    "gamification",
    "rpg",
    "hackathon"
  ],
  "author": "Team VitalNodes",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
